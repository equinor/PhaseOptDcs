<?xml version="1.0" encoding="utf-8"?>
<configuration xsi:noNamespaceSchemaLocation="PhaseOptDcs.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <OpcUrl>opc.tcp://localhost:4840/freeopcua/server/</OpcUrl>
  <OpcUser></OpcUser>
  <OpcPassword></OpcPassword>
  <DefaultNamespaceURI>http://karsto.sas.io</DefaultNamespaceURI>
  <Interval>1500.0</Interval>
  <ReadOnly>false</ReadOnly>
  <Streams>
    <Stream Name="Statpipe">
      <Composition SamplingInterval="2000">
        <Component Name="CO2" Id="1" Identifier="i=31" ScaleFactor="1" />
        <Component Name="N2" Id="2" RelativePath="PhaseOpt/Composition/N2" ScaleFactor="1" />
        <Component Name="CH4" Id="101" Identifier="i=33" ScaleFactor="1" />
        <Component Name="C2H6" Id="201" Identifier="i=34" ScaleFactor="1" />
        <Component Name="C3" Id="301" Identifier="i=35" ScaleFactor="1" />
        <Component Name="iC4" Id="401" Identifier="i=36" ScaleFactor="1" />
        <Component Name="nC4" Id="402" Identifier="i=37" ScaleFactor="1" />
        <Component Name="iC5" Id="503" Identifier="i=38" ScaleFactor="1" />
        <Component Name="nC5" Id="504" Identifier="i=39" ScaleFactor="1" />
        <Component Name="2-M-C5" Id="603" Identifier="i=40" ScaleFactor="1" />
        <Component Name="3-M-C5" Id="604" Identifier="i=41" ScaleFactor="1" />
        <Component Name="nC6" Id="605" Identifier="i=42" ScaleFactor="1" />
        <Component Name="C7P / nC7" Id="701" Identifier="i=43" ScaleFactor="1" />
        <Component Name="C7N / cy-C6" Id="606" Identifier="i=44" ScaleFactor="1" />
        <Component Name="C7A / Benzene" Id="608" Identifier="i=45" ScaleFactor="1" />
        <Component Name="C8P / nC8" Id="801" Identifier="i=46" ScaleFactor="1" />
        <Component Name="C8N / cy-C7" Id="707" Identifier="i=47" ScaleFactor="1" />
        <Component Name="C8A / Toluene" Id="710" Identifier="i=48" ScaleFactor="1" />
        <Component Name="C9P / nC9" Id="901" Identifier="i=49" ScaleFactor="1" />
        <Component Name="C9N / cy-C8" Id="806" Identifier="i=50" ScaleFactor="1" />
        <Component Name="C9A / m-xylene" Id="809" Identifier="i=51" ScaleFactor="1" />
        <Component Name="nC10" Id="1016" Identifier="i=52" ScaleFactor="1" />
      </Composition>
      <Cricondenbar>
        <Pressure Name="Pressure Name" Identifier="i=54" RelativePath="PhaseOpt/Cricondenbar/Pressure" Unit="barg" Type="double" Value="1000.0" />
        <Temperature Name="Temperature Name" Identifier="i=55" Unit="C" Type="double" Value="2683.0" />
      </Cricondenbar>
      <LiquidDropouts>
        <WorkingPoint Name="Kårstø1">
          <Pressure Name="Pressure Name" StartIdentifier="i=57" RelativePath="Pressure" Unit="barg" />
          <Temperature Name="Temperature Name" Identifier="i=59" Unit="C" />
          <DewPointMargin Name="Margin Name" Identifier="i=60" Type="double" />
          <DewPoint Name="DewPoint Name" Identifier="i=61" Unit="barg" Type="double" />
          <DropoutValue Name="DropoutValue" Identifier="s=dropout-id" type="double" />
          <DropoutPoint Name="DropoutPoint Name" Identifier="i=62" DropoutPercent="2.5" Unit="barg"
            Type="double" />
        </WorkingPoint>

        <WorkingPoint Name="Kårstø2" Raw="true">
          <!-- This should contain errors -->
          <Pressure Name="Pressure Name" Identifier="s=error-id" Unit="barg" />
          <Temperature Name="Temperature Name" RelativePath="error-path" Unit="C" />
          <DewPointMargin Name="Margin Name" Identifier="i=86" Type="double" />
          <DewPoint Name="DewPoint Name" Identifier="i=87" Unit="barg" Type="double" />
          <DropoutPoint Name="DropoutPoint Name" Identifier="i=88" DropoutPercent="2.5" Unit="barg"
            Type="double" />
        </WorkingPoint>
      </LiquidDropouts>
    </Stream>
  </Streams>
</configuration>