<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <OpcUrl>opc.tcp://localhost:62541/Quickstarts/DataAccessServer</OpcUrl>
  <OpcUser></OpcUser>
  <OpcPassword></OpcPassword>
  <Interval>20000.0</Interval>
  <Streams>
    <Stream Name="Statpipe">
      <Composition>
        <Component Name="CO2"  Id="1"    Value="0.023176439" ScaleFactor="1" />
        <Component Name="N2"   Id="2"    Value="0.006907786" ScaleFactor="1" />
        <Component Name="CH4"  Id="101"  Value="0.831210139" ScaleFactor="1" />
        <Component Name="C2H6" Id="201"  Value="0.077432143" ScaleFactor="1" />
		<Component Name="C3"   Id="301"  Value="0.038859448" ScaleFactor="1" />
        <Component Name="iC4"  Id="401"  Value="0.005465126" ScaleFactor="1" />
		<Component Name="nC4"  Id="402"  Value="0.009835248" ScaleFactor="1" />
		<Component Name="iC5"  Id="503"  Value="0.002111395" ScaleFactor="1" />
		<Component Name="nC5"  Id="504"  Value="0.002150455" ScaleFactor="1" />
        <Component Name="2-M-C5"          Id="603"     Value="0.000666455" ScaleFactor="1" />
		<Component Name="3-M-C5"          Id="604"     Value="0.000214464" ScaleFactor="1" />
		<Component Name="nC6"             Id="605"     Value="0.000498599" ScaleFactor="1" />
		<Component Name="C7P / nC7"       Id="701"     Value="0.000200478" ScaleFactor="1" />
		<Component Name="C7N / cy-C6"     Id="606"     Value="0.000647215" ScaleFactor="1" />
		<Component Name="C7A / Benzene"   Id="608"     Value="0.000163487" ScaleFactor="1" />
		<Component Name="C8P / nC8"       Id="801"     Value="4.15516E-05" ScaleFactor="1" />
		<Component Name="C8N / cy-C7"     Id="707"     Value="0.000254571" ScaleFactor="1" />
		<Component Name="C8A / Toluene"   Id="710"     Value="0.000100149" ScaleFactor="1" />
		<Component Name="C9P / nC9"       Id="901"     Value="1.68092E-05" ScaleFactor="1" />
		<Component Name="C9N / cy-C8"     Id="806"     Value="1.55526E-05" ScaleFactor="1" />
		<Component Name="C9A / m-xylene"  Id="809"     Value="2.39625E-05" ScaleFactor="1" />
		<Component Name="nC10"            Id="1016"    Value="8.52431E-06" ScaleFactor="1" />
      </Composition>
      <Cricondenbar>
        <Pressure    Name="Pressure Name"     Tag="ns=2;s=1:AI1001?Pressure"    Unit="barg" Type="single" />
        <Temperature Name="Temperature Name"  Tag="ns=2;s=1:AI1001?Temperature" Unit="C"    Type="single" />
      </Cricondenbar>
      <LiquidDropouts>
        <LiquidDropout>
          <WorkingPoint Name="Kårstø">
            <Pressure    Name="Pressure Name"    Tag="ns=2;s=1:PI1001?Measurement" Unit="barg" />
            <Temperature Name="Temperature Name" Tag="ns=2;s=1:TI1001?Measurement" Unit="C" />
            <Margin      Name="Margin Name"      Tag="ns=2;s=1:PI1002?Measurement"             Type="single" />
            <DewPoint    Name="DewPoint Name"    Tag="ns=2;s=1:PI1003?Measurement" Unit="barg" Type="single" />
            <DropoutPoint Name="DropoutPoint Name" Tag="" Unit="barg" Type="single" />
          </WorkingPoint>
        </LiquidDropout>
      </LiquidDropouts>
    </Stream>
    <Stream Name="Åsgard">
      <Composition>
        <Component Name="CO2" Id="1" Tag="ns=2;s=1:AI1002?K" ScaleFactor="1" />
        <Component Name="N2" Id="2" Tag="ns=2;s=1:AI1002?J" ScaleFactor="1" />
      </Composition>
    </Stream>
  </Streams>
</configuration>